{
	"name": "vasek-purchart/rabbit-mq-consumer-handler-bundle",
	"description": "Handle messages in RabbitMQ consumers in a safe and effective way",
	"license": "MIT",
	"authors": [
		{
			"name": "Va≈°ek Purchart",
			"email": "me@vasekpurchart.cz",
			"homepage": "http://vasekpurchart.cz"
		}
	],
	"require": {
		"php": "~7.2",
		"consistence/consistence": "~1.0|~2.0",
		"doctrine/orm": "~2.6",
		"php-amqplib/rabbitmq-bundle": "~1.14",
		"psr/log": "~1.0",
		"symfony/config": "~3.3|~4.0",
		"symfony/dependency-injection": "~3.3|~4.0",
		"symfony/http-kernel": "~3.3|~4.0",
		"symfony/yaml": "~3.3|~4.0"
	},
	"require-dev": {
		"consistence/coding-standard": "3.7",
		"matthiasnoback/symfony-dependency-injection-test": "3.1",
		"php-parallel-lint/php-console-highlighter": "1.0",
		"php-parallel-lint/php-parallel-lint": "1.3.2",
		"phing/phing": "2.16.1",
		"phpunit/phpunit": "7.5.1"
	},
	"autoload": {
		"psr-4": {
			"VasekPurchart\\RabbitMqConsumerHandlerBundle\\": "src"
		},
		"classmap": [ "src" ]
	},
	"autoload-dev": {
		"psr-4": {
			"VasekPurchart\\RabbitMqConsumerHandlerBundle\\": "tests"
		},
		"classmap": [ "tests" ]
	},
	"config": {
		"bin-dir": "bin"
	}
}
